{
  "network": "ClassifierResNet3dCSN2P1D",
  "encoder_params": {
    "encoder": "r152ip",
    "num_classes": 6,
    "pool": "max"
  },
  "multiplier": 2,
  "slice_size": 80,
  "crop_size": 256,
  "augmentations": "CropAugsFullRes",
  "dataset": {
    "type": "ClassifierDatasetCropsFullResLiverSpleenKidney",
    "params": {}
  },
  "optimizer": {
    "train_bs": 6,
    "val_bs": 1,
    "type": "AdamW",
    "weight_decay": 1e-2,
    "learning_rate": 0.0001,
    "schedule": {
      "type": "cosine",
      "mode": "step",
      "epochs": 50,
      "params": {
        "eta_min": 1e-5
      }
    }
  },
  "losses": [
    {
      "name": "BCE",
      "type": "BcePureLossCalc",
      "weight": 1.0,
      "display": true,
      "params": {}
    }
  ]
}